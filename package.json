{
  "name": "grafana-npm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && yarn build:main",
    "build:main": "tsc -p tsconfig.build.json",
    "lint:raw": "eslint --ext .ts --ext .js --ignore-pattern dist",
    "lint": "yarn lint:raw .",
    "lint-fix": "yarn lint --fix",
    "dev": "yarn ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.10",
    "@types/pouchdb": "^6.4.0",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": "^18.15",
    "yarn": "^1.22"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.11.2",
    "express": "^4.18.2",
    "mysql2": "^3.2.0",
    "p-queue": "^6.6.2",
    "pouchdb": "^7.2.2",
    "reissue": "^3.1.0",
    "sequelize": "^6.30.0",
    "underscore": "^1.13.6"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "yarn lint:raw --fix"
    ]
  }
}
