{
  "name": "grafana-npm",
  "version": "1.1.0-0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/julusian/grafana-npm-versions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julusian/grafana-npm-versions.git"
  },
  "bugs": {
    "url": "https://github.com/julusian/grafana-npm-versions/issues"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "postinstall:disable": "pinst --disable",
    "build": "rimraf dist && yarn build:main",
    "build:main": "tsc -p tsconfig.build.json",
    "lint:raw": "yarn eslint --ext .ts --ext .js --ignore-pattern dist",
    "lint": "yarn lint:raw .",
    "lint-fix": "yarn lint --fix",
    "dev": "yarn node --loader ts-node/esm src/index.ts",
    "eslint": "./node_modules/.bin/eslint",
    "prettier": "./node_modules/.bin/prettier",
    "lint-staged": "./node_modules/.bin/lint-staged"
  },
  "devDependencies": {
    "@sofie-automation/code-standard-preset": "^2.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pouchdb": "^6.4.2",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "~4.9.5"
  },
  "engines": {
    "node": "^20.11"
  },
  "packageManager": "yarn@4.1.0",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "express": "^4.18.3",
    "mysql2": "^3.9.2",
    "p-queue": "^8.0.1",
    "pouchdb": "^8.0.1",
    "reissue": "^3.1.0",
    "sequelize": "^6.37.1"
  },
  "prettier": "@sofie-automation/code-standard-preset/.prettierrc.json",
  "lint-staged": {
    "*.{json,md}": [
      "yarn prettier --write"
    ],
    "*.{ts,js}": [
      "yarn lint:raw --fix"
    ]
  }
}
